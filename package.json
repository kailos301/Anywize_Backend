{
  "name": "anywize",
  "version": "0.5.3",
  "engines": {
    "node": ">=14.0.0",
    "yarn": "1.x.x"
  },
  "license": "ISC",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "npm run clean && nodemon",
    "clean": "rimraf dist",
    "handleEnvBefore": "node handleWindowsEnv.js -r dotenv/config -e dotenv_config_path=.test.env",
    "handleEnv": "ts-mocha --require dotenv/config dotenv_config_path=.test.env --require ./src/index.ts 'src/tests/**/*.{js,ts}' -t 70000 --exit",
    "build": "npm run clean && mkdir -p dist && node ./node_modules/esbuild-node-tsc/dist/index --config=esbuild.config.js && npm run apidoc",
    "test": "yarn truncateTestDatabase && yarn dbMigrateTest && NODE_ENV=test ts-mocha --require dotenv/config dotenv_config_path=.test.env --require ./src/index.ts 'src/tests/**/*.{js,ts}' -t 70000 --exit",
    "runTest": "NODE_ENV=test ts-mocha --require dotenv/config dotenv_config_path=.test.env --require ./src/index.ts 'src/tests/**/*.{js,ts}' -t 70000 --exit",
    "test:coverage": "yarn truncateTestDatabase && yarn dbMigrateTest && NODE_ENV=test nyc ts-mocha --require dotenv/config dotenv_config_path=.test.env --require ./src/index.ts 'src/tests/**/*.{js,ts}' -t 70000 --exit",
    "dbMigrateTest": "node -r dotenv/config -e 'require(\"./postgrator/postgrator.js\").migrate()' dotenv_config_path=.test.env",
    "truncateTestDatabase": "node -r dotenv/config -e 'require(\"./postgrator/postgrator.js\").truncateDatabase()' dotenv_config_path=.test.env",
    "apidoc": "node ./node_modules/apidoc/bin/apidoc -f \"./src/routes/import.ts\" -o apidoc/",
    "clean:db": "DEBUG=anywize* NODE_ENV=test ts-node --transpile-only --require dotenv/config ./src/fix-customers"
  },
  "dependencies": {
    "@sentry/node": "^6.11.0",
    "@sentry/tracing": "^6.11.0",
    "apidoc": "^0.28.1",
    "aws-sdk": "^2.936.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "debug": "^4.3.0",
    "ejs": "^3.1.6",
    "esbuild-node-tsc": "^1.2.0",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "getenv": "^1.0.0",
    "html-pdf": "^3.0.1",
    "html-pdf-node": "^1.0.7",
    "http-errors": "^1.8.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "mime-types": "^2.1.31",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.1",
    "nodemailer": "^6.6.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "postgrator": "^4.1.1",
    "randomstring": "^1.2.1",
    "rimraf": "^3.0.2",
    "sequelize": "^6.6.2",
    "socket.io": "^4.2.0",
    "uuid": "^8.3.2",
    "write-excel-file": "^1.4.13"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^8.2.1",
    "@types/multer": "^1.4.6",
    "@types/node": "^14.14.31",
    "@types/sequelize": "^4.28.9",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.15.2",
    "chai": "^4.3.4",
    "codecov": "3.8.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-watch": "^7.0.0",
    "faker": "^5.5.3",
    "mocha": "^8.3.2",
    "nock": "^13.0.5",
    "nodemon": "^2.0.3",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "supertest": "^6.1.3",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "directories": {
    "test": "test"
  },
  "apidoc": {
    "name": "Anywize",
    "description": "Anywize API",
    "sampleUrl": null
  },
  "nyc": {
    "reporter": [
      "lcov"
    ],
    "exclude": [
      "src/tests/*.ts"
    ]
  }
}
